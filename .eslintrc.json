{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-console": 1,
		"no-empty": 1,
		"no-unused-vars": 1,
		"no-use-before-define": 2,
		"no-useless-constructor": "warn",
		"prefer-const": "warn"
	}
}
